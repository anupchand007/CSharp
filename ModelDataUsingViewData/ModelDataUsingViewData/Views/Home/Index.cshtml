//when we have multiple objects


<h1>1. ViewData Method</h1>

@{
    if(ViewDataEmp2 != null)
    {
        foreach (var emp in ViewDataEmp2)
        {
            <h2>Emp_ID: @emp.EmpId</h2>
            <h2>Emp_Name: @emp.Name</h2>
            <h2>Emp_Salary: @emp.salary</h2>
    }
    }
    
}

<h1>2. ViewBag Method</h1>
@{
    foreach (var emp in ViewBagEmp2)
    {
        <h2>Emp_ID: @emp.EmpId</h2>
        <h2>Emp_Name: @emp.Name</h2>
        <h2>Emp_Salary: @emp.salary</h2>
    }
}

<h1>3. TempData Method</h1>
@{
    foreach (var emp in TempDataEmp2)
    {
        <h2>Emp_ID: @emp.EmpId</h2>
        <h2>Emp_Name: @emp.Name</h2>
        <h2>Emp_Salary: @emp.salary</h2>
    }
}

