    //when usign one than on Object
    var ViewDataEmp2 = ViewData["myEmployee"] as List<Employee>; 
    var ViewBagEmp2 = ViewBag.myEmployee;
    var TempDataEmp2 = TempData["myEmployee"] as List<Employee>;
}

