@* @model Employee //Strongly Type View //when usign single object *@
@* //OR *@
@* @model IEnumerable<StronglyTypedViewASPCore.Models.Employee> //used when there are multiple objects or list *@
@model IEnumerable<Employee>
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://learn.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>

@* 
<h2>@Model.Empid</h2>
<h2>@Model.Name</h2>
<h2>@Model.Salary</h2>
 *@

@{
    foreach (var emp in Model)
    {
        <h2>@emp.Empid</h2>
        <h2>@emp.Name</h2>
        <h2>@emp.Salary</h2>
    }
}